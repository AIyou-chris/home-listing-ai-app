# Environment Configuration for Home Listing AI App
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google AI Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# App Configuration
VITE_APP_NAME=Home Listing AI
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Mailgun Email (Optional)
MAILGUN_API_KEY=your_mailgun_api_key
MAILGUN_DOMAIN=mg.yourdomain.com
MAILGUN_FROM_EMAIL=HomeListingAI <noreply@yourdomain.com>

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_VOICE_ASSISTANT=true
VITE_ENABLE_NOTIFICATIONS=true

# API Endpoints
VITE_API_BASE_URL=https://your-api-domain.com

# Rebrandly Link Shortening (Optional)
VITE_REBRANDLY_API_KEY=your_rebrandly_api_key
VITE_REBRANDLY_WORKSPACE_ID=your_rebrandly_workspace_id
VITE_REBRANDLY_DOMAIN=links.yourbrand.com

