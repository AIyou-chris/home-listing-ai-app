# Environment Configuration for Home Listing AI App
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Default agent id for new leads (auth.users.id)
DEFAULT_LEAD_USER_ID=replace_with_default_agent_uuid

# Application URLs
DASHBOARD_BASE_URL=https://aiyouagent.com
APP_BASE_URL=http://localhost:5173

# Transactional Email Providers (configure at least one)
RESEND_API_KEY=
POSTMARK_SERVER_TOKEN=
SENDGRID_API_KEY=
EMAIL_FROM_NAME="AI You Agent Team"
EMAIL_FROM_ADDRESS="AI You Agent Team <noreply@aiyouagent.com>"
SUPPORT_EMAIL=support@aiyouagent.com

# Google AI Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Google Cloud Translation (optional for auto language detection)
TRANSLATE_API_KEY=your_google_translate_api_key

# App Configuration
VITE_APP_NAME=Home Listing AI
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Mailgun Email (Optional)
MAILGUN_API_KEY=your_mailgun_api_key
MAILGUN_DOMAIN=mg.yourdomain.com
MAILGUN_FROM_EMAIL=HomeListingAI <noreply@yourdomain.com>

# Google OAuth (Gmail connection)
GOOGLE_OAUTH_CLIENT_ID=your_google_client_id
GOOGLE_OAUTH_CLIENT_SECRET=your_google_client_secret
GOOGLE_OAUTH_REDIRECT_URI=http://localhost:3002/api/email/google/oauth-callback

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_VOICE_ASSISTANT=true
VITE_ENABLE_NOTIFICATIONS=true

# API Endpoints
VITE_API_BASE_URL=https://your-api-domain.com

# Payments
STRIPE_SECRET_KEY=sk_test_your_stripe_secret
STRIPE_PRICE_ID=price_your_price_id
STRIPE_PRODUCT_NAME=AI You Agent Subscription
STRIPE_CURRENCY=usd
STRIPE_DEFAULT_AMOUNT_CENTS=5900

PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
PAYPAL_ENV=sandbox
PAYPAL_CURRENCY=USD

# Admin API
ADMIN_API_KEY=change-this-value

